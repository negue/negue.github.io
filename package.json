    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "babel-plugin-styled-components": "^1.10.3",
    "concurrently": "^4.1.1",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.0.1",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2",
    "typescript-styled-plugin": "^0.14.0"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop --host=0.0.0.0",
    "format": "prettier --write '{__mocks__,src,test}/**/*.{js,ts,tsx}'",
    "lint": "eslint src/**/*.{js,ts,tsx}",
    "lint:fix": "yarn run lint --fix",
    "start": "concurrently \"gatsby develop\" \"yarn run codegen\"",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "codegen": "graphql-codegen --config graphql-codegen.yml",
    "graphql-codegen": "graphql-codegen",
    "markdown:lint": "npx markdownlint-cli@0.19 -c tools/configs/markdownlint.yml **/*.md -i node_modules -i dist"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ignore-path .gitignore --ignore-path .prettierignore --fix",
      "git add"
    ],
    "*.{md,yaml,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.svg": [
      "svgo --pretty --indent=2 --config=svgo.yml --multipass",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "//pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "gatsby",
    "typescript",
    "graphql"
  ],
  "license": "MIT"
}
